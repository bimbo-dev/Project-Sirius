@model IEnumerable<Sirius.Services.DTO.UserDTO>


<p class="lead">Team Members</p>
<ul class="list-inline team-list">
    @if (Model.Count() != 0)
    {
        @foreach (var member in Model)
        {
            <li>
                <a href="#" data-toggle="tooltip" data-placement="top" title="@member.FullName">
                    <img src="@Url.Action("GetUserIdAvatar", "Base", new { userId = member.UserId })" alt="User Image" class="img-circle img-height">
                </a>
            </li>
        }
    }
</ul><!-- /.users-list -->